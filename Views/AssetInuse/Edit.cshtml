<script type="text/javascript">
    var d = new Date();
        $("#btnGet").click(function() {

            var AssetCode = $("#AssetCode").val();
            var StickerName = $('#StickerName').val();
            var AssetName = $("#AssetName").val();

            var Location = $('#LocationDropdown').val();
            var Department = $('#Department').val();
            var Owner = $('#OwnerDropdown').val();

            var CostCenter = $('#CostDropdown').val();
            var RefCode = $('#RefCode').val();

            var PurchaseDate = $('#PurchaseDate').val();
            var UseDate = $('#UseDate').val();
            var ExpireDate = $('#ExpireDate').val();
            var WarrantyDate = $('#WarrantyDate').val();
            var NextServiceDate = $('#NextServiceDate').val();
            var DisposalDate = $('#DisposalDate').val();

            var Building = $('#Building').val();
            var Floor = $('#Floor').val();
            var Room = $('#Room').val();
            var Insured = $('#Insured').val();
            var UnderMaintenance = $('#UnderMaintenance').val();


            var snedData = { ItemID: AssetCode, NameTH: AssetName, NameEN: StickerName, CompanyID: Location, Department: Department, OwnerID: Owner, CostCenter: CostCenter, UseDate: UseDate, PurchaseDate: PurchaseDate, ExpireDate: ExpireDate, WarrantyDate: WarrantyDate, NextServiceDate: NextServiceDate, DisposalDate: DisposalDate, IsInsured: Insured, IsUnderMaintenance: UnderMaintenance, Building: Building, Floor: Floor, Room: Room, RefID: RefCode }



            $.ajax({
                type: "POST",
                url: "/api/New",
                data: snedData,

                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function() {
                    alert("Succuss " );

                },
                failure: function(response) {
                    alert(response.data);
                },
                error: function(response) {
                    window.location.href = "/AssetInuse/Inuse";
                }

            });
            
        });


</script>
<script>
    $(document).ready(function () {
        $.ajax({
            type: "GET",
            url: "/api/getAlllocation",
            data: '{}',
            success: function (data) {
                var s = '<option value="-1"></option>';
                var c = '<option value="-1"></option>';

                for (var i = 0; i < data.length; i++) {
                    if (data[i].ParentID!=="") {
                        c += '<option value="' + data[i].ParentID + '">' + data[i].ParentID + ' | ' + data[i].Name1 + '</option>';
                        s += '<option value="' + data[i].CompanyID + '">' + data[i].CompanyID + ' | ' + data[i].Name1 + '</option>';
                    }
                }
                $("#LocationDropdown").html(s);
                $("#CostDropdown").html(c);
            }
        });



        $.ajax({
            type: "GET",
            url: "/api/getOwner",
            data: "{}",
            success: function(data) {
                var s = '<option value="-1"></option>';
                for (var i = 0; i < data.length; i++) {
                    if (data[i].Username !== "") {
                        s += '<option value="' + data[i].Username + '">' + data[i].Username + ' | ' + data[i].FullName + '</option>';
                    }

                }
                $("#OwnerDropdown").html(s);

            }

        });

        var id = window.location.pathname.split("/")[3];

        $.ajax({
            type: "GET",
            url: "/api/getid/",
            data: {
                id: id
    },
            success: function (data) {
                console.log(data);
                debugger;


                function leadingZero(value) {
                    if (value < 10) {
                        return "0" + value.toString();
                    }
                    return value.toString();
                }


                function formatDate(date) {
                    var dateformat = new Date(date);
                    return dateformat.getFullYear() + "-" + leadingZero(dateformat.getMonth()) + "-" + leadingZero(dateformat.getDay());
                }
                document.getElementById("AssetCode").value = data[0].ItemID;
                document.getElementById("AssetName").value = data[0].NameTH;
                document.getElementById("LocationDropdown").value = data[0].CompanyID;
                document.getElementById("StickerName").value = data[0].NameEN;
                document.getElementById("Department").value = data[0].Department;
                document.getElementById("OwnerDropdown").value = data[0].OwnerID;
                document.getElementById("RefCode").value = data[0].RefID;
                document.getElementById("CostDropdown").value = data[0].CostCenter;
                document.getElementById("PurchaseDate").value = formatDate(data[0].PurchaseDate);
                document.getElementById("UseDate").value = formatDate(data[0].UseDate);
                document.getElementById("ExpireDate").value = formatDate(data[0].ExpireDate);
                document.getElementById("WarrantyDate").value = formatDate(data[0].WarrantyDate);
                document.getElementById("NextServiceDate").value = formatDate(data[0].NextServiceDate);
                document.getElementById("DisposalDate").value = formatDate(data[0].DisposalDate);
                document.getElementById("Building").value = data[0].Building;
                document.getElementById("Floor").value = data[0].Floor;
                document.getElementById("Room").value = data[0].Room;
                document.getElementById("Insured").value = data[0].IsInsured;
                document.getElementById("UnderMaintenance").value = data[0].IsUnderMaintenance;

            },
            error:function(data) {

            }
        });

        $('#PrintQRCode').click(function () {
            alert($("#AssetCode").val());
            window.location.href = "/QRCode/PrintQRCode/" + $("#AssetCode").val();


        });

    });


</script>